{
    "name":"Color picker",
    "description":"Extension for picking color from any website",
    "version":"1.0.1",
    "manifest_version":3,
    "background":{
        "service worker":"background.js"
    },
    "permissions":[
        "activeTab",
        "scripting"

    ],
    "action":{
        "default_popup":"popup.html"

    },
    "icons":{
        "16":"/images/picker.png",
        "32":"/images/picker_32.png",
        "48":"/images/picker_48.png"
    }
}